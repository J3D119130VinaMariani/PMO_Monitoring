@using PMO_Monitoring.viewmodel
@model VMTrxProject

@{
    Layout = null;
}
<form class="mt-4" id="edit">

    <div class="row">
        <div class="col-md-6">
            <input type="hidden" class="form-control" id="Id" name="Id" value="@Model.Id" readonly>
            <div class="mb-3 row">
                <label for="Projectname" class="col-sm-4 col-from-label">Project Name</label>
                <div class="col-sm-8">
                    <input type="text" class="form-control" id="Projectname" name="Projectname" value="@Model.Projectname" readonly></input>
                </div>
            </div>
            <div class="mb-3 row">
                <label for="Divisiid" class="col-sm-4 col-from-label">Divisi Id</label>
                <div class="col-sm-8">
                    <input type="text" class="form-control" id="Divisiid" name="Divisiid" value="@Model.Divisiid" readonly></input>

                </div>
            </div>
            <div class="mb-3 row">
                <label for="NamaDivisi" class="col-sm-4 col-from-label">Nama Divisi</label>
                <div class="col-sm-8">
                    <input type="text" class="form-control" id="NamaDivisi" name="NamaDivisi" value="@Model.NamaDivisi" readonly></input>

                </div>
            </div>
            <div class="mb-3 row">
                <label for="Keterangan" class="col-sm-4 col-from-label">Keterangan</label>
                <div class="col-sm-8">
                    <input type="text" class="form-control" id="Keterangan" name="Keterangan" value="@Model.Keterangan" readonly></input>

                </div>
            </div>
        </div>
        <div class="col-md-6">
            <div class="mb-3 row">
                <label for="Createdby" class="col-sm-4 col-from-label">CreateBy</label>
                <div class="col-sm-8">
                    <input type="text" class="form-control" id="Createdby" name="Createdby" value="@Model.Createdby" readonly></input>
                </div>

            </div>
            <div class="mb-3 row">
                <label for="Createddate" class="col-sm-4 col-from-label">CreateDate</label>
                <div class="col-sm-8">
                    <input type="text" class="form-control" id="Createddate" name="Createddate" value="@Model.Createddate" readonly></input>
                </div>
            </div>
            <div class="mb-3 row">
                <label for="Lastupdatedby" class="col-sm-4 col-from-label">Last Updatedby</label>
                <div class="col-sm-8">
                    <input type="text" class="form-control" id="Lastupdatedby" name="Lastupdatedby" value="@Model.Lastupdatedby" readonly></input>
                </div>

            </div>
            <div class="mb-3 row">
                <label for="Lastupdateddate" class="col-sm-4 col-from-label">Last Updateddate</label>
                <div class="col-sm-8">
                    <input type="text" class="form-control" id="Lastupdateddate" name="Lastupdateddate" value="@Model.Lastupdateddate" readonly></input>
                </div>
            </div>
        </div>
    </div>
</form>
